<h2>Add S3 Object for &quot;<%= @product.name %>&quot;</h2>
<% form_for :s3_product, :url => admin_product_s3_product_path(@product), :html => {:method => :put} do |f| %>
	<%= render :partial => 'form', :locals => {:f => f} %>
	<%= submit_tag "Update" %> | <%= link_to "Back", admin_product_s3_products_path(@product) %>
<% end %>